{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "bun src/cli.ts",
    "build": "npm run build:linux-x64 && npm run build:windows-x64",
    "build:linux-x64": "npm run build:base -- --target=bun-linux-x64",
    "build:windows-x64": "npm run build:base -- --target=bun-windows-x64",
    "build:current": "npm run build:base",
    "build:base": "bun build  ./src/cli.ts --compile --minify --sourcemap --outfile \"dist/wplace_archiver\""
  },
  "bin": {
    "app": "src/cli.ts"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/humanize-duration": "^3.27.4",
    "@types/victor": "^1.1.5",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@inquirer/confirm": "^5.1.14",
    "@inquirer/prompts": "^7.8.1",
    "chalk": "^5.5.0",
    "commander": "^14.0.0",
    "fetch-retry": "^6.0.0",
    "fs-extra": "^11.3.1",
    "humanize-duration": "^3.33.0",
    "humanize-number": "^0.0.2",
    "neverthrow": "^8.2.0",
    "node-fetch": "^3.3.2",
    "p-queue": "^8.1.0",
    "pkg": "^5.8.1",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.34.3",
    "tsx": "^4.20.3",
    "victor": "^1.1.0",
    "zod": "^4.0.17"
  }
}